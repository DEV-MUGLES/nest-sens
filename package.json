{
  "name": "nest-sens",
  "version": "0.1.2",
  "description": "NCP SENS Client component for Nest.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DEV-MUGLES/nest-sens.git"
  },
  "keywords": [
    "nestjs",
    "sens",
    "naver",
    "sms",
    "kakaotalk",
    "alimtalk"
  ],
  "author": "DEV-MUGLES",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DEV-MUGLES/nest-sens/issues"
  },
  "homepage": "https://github.com/DEV-MUGLES/nest-sens#readme",
  "scripts": {
    "build": "rimraf -rf dist && tsc -p tsconfig.json",
    "format": "prettier --write **/*.{ts,json} --ignore-path ./.prettierignore",
    "lint": "eslint \"lib/**/*.ts\" --fix",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "prepublish:npm": "npm run format && npm run lint && npm run build",
    "publish:npm": "npm publish --access public",
    "prerelease": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "coveralls": "npm run coverage --coverageReporters=text-lcov | coveralls"
  },
  "peerDependencies": {
    "@nestjs/common": "^6.10.11 || ^7.0.0",
    "@nestjs/core": "^6.10.11 || ^7.0.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.0.0"
  },
  "dependencies": {
    "@pickk/sens": "^0.2.0"
  },
  "devDependencies": {
    "@nestjs/common": "^7.6.15",
    "@nestjs/core": "^7.6.15",
    "@nestjs/testing": "^7.6.15",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.41",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.1.0",
    "ts-jest": "^26.5.5",
    "typescript": "^4.2.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage"
  }
}
