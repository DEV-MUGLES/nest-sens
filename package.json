{
  "name": "nest-sens",
  "version": "1.1.0",
  "description": "NCP SENS Client component for Nest.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DEV-MUGLES/nest-sens.git"
  },
  "keywords": [
    "nestjs",
    "sens",
    "naver",
    "sms",
    "kakaotalk",
    "alimtalk"
  ],
  "author": "DEV-MUGLES",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DEV-MUGLES/nest-sens/issues"
  },
  "homepage": "https://github.com/DEV-MUGLES/nest-sens#readme",
  "scripts": {
    "build": "rimraf -rf dist && tsc -p tsconfig.json",
    "format": "prettier --write **/*.{ts,json} --ignore-path ./.prettierignore",
    "lint": "eslint \"lib/**/*.ts\" --fix",
    "type-check": "tsc --noEmit",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "prepublish:npm": "npm run format && npm run lint && npm run build",
    "publish:npm": "npm publish --access public",
    "prerelease": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "coveralls": "npm run coverage --coverageReporters=text-lcov | coveralls"
  },
  "peerDependencies": {
    "@nestjs/common": "*",
    "@nestjs/core": "*",
    "reflect-metadata": "*",
    "rxjs": "*"
  },
  "dependencies": {
    "@pickk/sens": "^1.1.0"
  },
  "devDependencies": {
    "@nestjs/common": "8.0.7",
    "@nestjs/core": "8.0.7",
    "@nestjs/testing": "8.0.7",
    "@types/jest": "27.0.2",
    "@types/node": "14.17.20",
    "@typescript-eslint/eslint-plugin": "4.32.0",
    "@typescript-eslint/parser": "4.32.0",
    "coveralls": "3.1.1",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.24.2",
    "jest": "27.2.4",
    "prettier": "2.4.1",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.3.0",
    "ts-jest": "27.0.5",
    "typescript": "4.4.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage"
  }
}
